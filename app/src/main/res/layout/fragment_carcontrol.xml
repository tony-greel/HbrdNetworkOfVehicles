<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">




    <RelativeLayout
        android:elevation="10dp"
        android:outlineProvider="bounds"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/img_my_car_background">

        <ImageView
            android:layout_width="12dp"
            android:layout_height="120dp"
            android:background="@drawable/img_carcontrol_statistics"
            android:layout_marginLeft="22dp"
            android:layout_marginTop="70dp"/>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp">
            <ImageView
                android:layout_width="4dp"
                android:layout_height="150dp"
                android:background="@drawable/img_carcontrol_statistics"
                android:layout_marginTop="40dp"/>
            <ImageView
                android:layout_width="9dp"
                android:layout_height="13dp"
                android:background="@drawable/img_car_control_f"
                android:layout_marginTop="40dp"
                android:layout_marginLeft="10dp"/>
            <ImageView
                android:layout_width="15dp"
                android:layout_height="13dp"
                android:background="@drawable/img_car_control_oil_quantity"
                android:layout_marginTop="110dp"
                android:layout_marginLeft="9dp"/>
            <ImageView
                android:layout_width="9dp"
                android:layout_height="13dp"
                android:background="@drawable/img_carcontrol_statistics_tow"
                android:layout_marginTop="177dp"
                android:layout_marginLeft="11dp"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="60"
                android:textSize="50dp"
                android:textColor="#FFFFFF"
                android:layout_marginTop="20dp"
                android:layout_centerHorizontal="true"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:text="Km/h"
                android:textColor="#FFFFFF"
                android:layout_marginTop="75dp" />
        </RelativeLayout>


        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="110dp"
            android:layout_centerHorizontal="true">

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:background="@drawable/img_1_1"
                android:layout_marginRight="170dp"
                android:layout_marginTop="13dp" />

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:background="@drawable/img_2_1"
                android:layout_marginRight="170dp"
                android:layout_marginTop="39dp" />

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:background="@drawable/img_3_1"
                android:layout_marginRight="170dp"
                android:layout_marginTop="65dp" />

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:background="@drawable/img_1_2"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="13dp" />

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:background="@drawable/img_2_2"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="39dp" />

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:background="@drawable/img_3_2"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="65dp" />

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:background="@drawable/img_1_3"
                android:layout_marginLeft="60dp"
                android:layout_marginTop="13dp" />

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:background="@drawable/img_2_3"
                android:layout_marginLeft="60dp"
                android:layout_marginTop="39dp" />

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:background="@drawable/img_3_3"
                android:layout_marginLeft="60dp"
                android:layout_marginTop="65dp" />


            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:background="@drawable/img_1_4"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="13dp" />

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:background="@drawable/img_2_4"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="39dp" />

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:background="@drawable/img_3_4"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="65dp" />

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:background="@drawable/img_1_5"
                android:layout_marginLeft="150dp"
                android:layout_marginTop="13dp" />

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:background="@drawable/img_2_5"
                android:layout_marginLeft="150dp"
                android:layout_marginTop="39dp" />

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:background="@drawable/img_3_5"
                android:layout_marginLeft="150dp"
                android:layout_marginTop="65dp" />

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:background="@drawable/img_1_6"
                android:layout_marginLeft="180dp"
                android:layout_marginTop="13dp" />

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:background="@drawable/img_2_6"
                android:layout_marginLeft="180dp"
                android:layout_marginTop="39dp" />

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:background="@drawable/img_3_6"
                android:layout_marginLeft="180dp"
                android:layout_marginTop="65dp" />


        </RelativeLayout>


        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="38dp">
            <ImageView
                android:layout_width="4dp"
                android:layout_height="150dp"
                android:background="@drawable/img_carcontrol_statistics"
                android:layout_marginTop="40dp"
                android:layout_marginLeft="60dp"/>
            <ImageView
                android:layout_width="11dp"
                android:layout_height="13dp"
                android:background="@drawable/img_car_control_h"
                android:layout_marginTop="40dp"
                android:layout_marginLeft="44dp"/>
            <ImageView
                android:layout_width="11dp"
                android:layout_height="14dp"
